<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.syj.spring.test.mybatis.repository.RealEstateRepository">
 
 	<select id="selectRealEstate"
 	resultType="com.syj.spring.test.mybatis.domain.RealEstate">
 		SELECT
 			`id`
 			,`realtorId`
 			,`address`
 			,`area`
 			,`type`
 			,`price`
 			,`rentPrice`
 				,`createdAt`
 			,`updatedAt`
 		FROM
 			`real_estate`
 		WHERE
 			`id` = #{id}
 	</select>
 
 
 </mapper>