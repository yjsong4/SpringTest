<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.syj.spring.test.jsp.repository.SellerRepository">
 
 	<insert id="insertSeller"
 	parameterType="map">
 		INSERT INTO
 			`seller`
 		(
 			`nickname`
 			, `temperature`
 			, `profileImage`
 		)
 		VALUE
 		(
 			#{nickname}
 			, #{temperature}
 			, #{profileImage}
 		)
 	</insert>
 
 
 </mapper>